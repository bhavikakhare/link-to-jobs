<h1>Job Postings</h1>

<table class="table table-hover">

  <thead class="thead-dark">
    <tr>
      <th>Title</th>
      <th>Category</th>
      <th>Company</th>
      <th> </th>
    </tr>
  </thead>

  <tbody>
    <% @job_postings.each do |jp| %>
      <tr>
        <td><%= link_to jp.title, job_posting_path(jp) %></td>
        <td><%= jp.job_category %></td>
        <td><%= link_to jp.company.name, company_path(jp) %></td>
        <td>
        </td>
    <% end %>
  </tbody>

</table>